{
  "name": "DigiFin",
  "version": "0.1.0",
  "private": true,
  "description": "Online Banking Application",
  "author": "@joekenpat",
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@aws-amplify/interactions": "^6.0.25",
    "@aws-amplify/ui-components": "^1.9.40",
    "@aws-amplify/ui-vue": "^4.2.3",
    "@capacitor/android": "^1.3.0",
    "@capacitor/cli": "^1.3.0",
    "@capacitor/core": "^1.3.0",
    "@mdi/font": "^3.6.95",
    "aws-amplify": "^6.0.25",
    "axios": "^0.19.0",
    "babel-loader": "^9.1.3",
    "core-js": "^3.3.2",
    "moment": "^2.24.0",
    "print-js": "^1.0.61",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "v-offline": "^1.1.0",
    "vue": "^2.6.10",
    "vue-barcode": "^1.2.0",
    "vue-router": "^3.1.3",
    "vue-uuid": "^1.1.1",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.1.1",
    "vuex-persistedstate": "^2.7.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.24.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "electron": "^6.0.0",
    "electron-icon-builder": "^1.0.1",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-vuetify": "^1.1.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": [
        "off",
        {
          "endOfLine": "auto"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT"
}
